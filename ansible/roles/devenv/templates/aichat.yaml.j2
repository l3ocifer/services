# aichat configuration
clients:
  - type: ollama
    models:
      - name: {{ ai_model_name }}
        max_input_tokens: 128000
        supports_function_calling: true
    extra:
      api_base: http://localhost:11434/api

model: {{ ai_model_name }}
provider: ollama
stream: true
save: true
theme:
  name: {{ ai_theme | default('dark') }}
