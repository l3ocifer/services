"*":
  core:
    telemetryConsent: "no"
    themes: [
      "one-dark-ui"
      "one-dark-syntax"
    ]
  editor:
    softWrap: true
    softWrapAtPreferredLineLength: true
    preferredLineLength: 120
    showInvisibles: false
    tabLength: 2
  "exception-reporting":
    userId: "{{ pulsar_user_id | default('') }}" 